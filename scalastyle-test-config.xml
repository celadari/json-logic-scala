      <parameter name="maxLineLength">
        <![CDATA[160]]>
      </parameter>
      <parameter name="tabSize">
        <![CDATA[4]]>
      </parameter>
      <parameter name="ignoreImports">
        <![CDATA[false]]>
      </parameter>
    </parameters>
  </check>
  <check class="org.scalastyle.scalariform.ClassNamesChecker" level="warning" enabled="true">
    <parameters>
      <parameter name="regex">
        <![CDATA[^[A-Z][A-Za-z0-9]*$]]>
      </parameter>
    </parameters>
  </check>
  <check class="org.scalastyle.scalariform.ObjectNamesChecker" level="warning" enabled="true">
    <parameters>
      <parameter name="regex">
        <![CDATA[^[A-Z][A-Za-z0-9]*$]]>
      </parameter>
    </parameters>
  </check>
  <check class="org.scalastyle.scalariform.PackageNamesChecker" level="warning" enabled="true">
    <parameters>
      <parameter name="regex">
        <![CDATA[^[a-z]+$]]>
      </parameter>
    </parameters>
  </check>
  <check class="org.scalastyle.scalariform.PackageObjectNamesChecker" level="warning" enabled="true">
    <parameters>
      <parameter name="regex">
        <![CDATA[^[a-z][A-Za-z]*$]]>
      </parameter>
    </parameters>
  </check>
  <check class="org.scalastyle.scalariform.EqualsHashCodeChecker" level="warning" enabled="true" />
