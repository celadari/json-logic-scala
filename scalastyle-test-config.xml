      <parameter name="maxFileLength">
        <![CDATA[800]]>
      </parameter>
    </parameters>
  </check>
  <check class="org.scalastyle.file.HeaderMatchesChecker" level="warning" enabled="true">
    <parameters>
      <parameter name="header">
        <![CDATA[// Copyright 2019 celadari. All rights reserved. MIT license.]]>
      </parameter>
      <parameter name="regex">
        <![CDATA[false]]>
      </parameter>
    </parameters>
  </check>
  <check class="org.scalastyle.scalariform.SpacesAfterPlusChecker" level="warning" enabled="true" />
